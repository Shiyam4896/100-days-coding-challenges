def print_perm(x: int, y: int, z: int, n: int):
    list = []
    for i in range(x+1):
        for j in range(y+1):
            for k in range(z+1):
                if i+j+k == n:
                    continue
                else:
                    list.append([i, j, k])
    print(list)
    
if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
print_perm(x, y, z, n)
